<PosScreen>:
    Navigation:


<Navigation>:
    nav:nav_drawer
    ScreenManager:
        id:screenmanager
        Screen:
            id:screen0
            name:"screen0"
            manager:screenmanager
            BoxLayout:
                orientation: 'vertical'

                MDToolbarKu:
                    # title: "Navigation Drawer"
                    # elevation: 10
                    # left_action_items: [['menu', lambda x: nav_drawer.toggle_nav_drawer()]]
                    # right_action_items: [["dots-vertical", lambda x: self.parent.parent.parent.parent.parent.popup()]]
               
                Widget:
                MDRectangleFlatButton:
                    text:"Prees here"
                    on_release: self.parent.parent.parent.parent.parent.parent.hello()

                    # on_release: root.ids.screenmanager.current="screen1"

        Screen1:
        Screen2:
        Screen3:

    MDNavigationDrawer:
        id: nav_drawer
        ContentNavigationDrawer:



<ContentNavigationDrawer@BoxLayout>:
    orientation: "vertical"
    padding: "8dp"
    spacing: "8dp"

    AnchorLayout:
        anchor_x: "left"
        size_hint_y: None
        height: avatar.height

        Image:
            id: avatar
            size_hint: None, None
            size: "56dp", "56dp"
            source: "kivymd.png"

    MDLabel:
        text: "KivyMD library"
        font_style: "Button"
        size_hint_y: None
        height: self.texture_size[1]

    MDLabel:
        text: "kivydevelopment@gmail.com"
        font_style: "Caption"
        size_hint_y: None
        height: self.texture_size[1]

    MDRectangleFlatButton:
        text:"this is screen 1"
        on_release:'screen1'

    List:

        # DrawerList:
        #     id: md_list


<List@ScrollView>:
    ScrollView:
        MDList:
            OneLineAvatarIconListItem:
                on_release: print("Click!")
                text:"Beranda"
                IconLeftWidget:
                    icon: "home"

            OneLineAvatarIconListItem:
                on_release: print("Click 2!")
                text:"Penjualan"
                IconLeftWidget:
                    icon: "account"


<Screen1@Screen>:
    name:"screen1"
    MDRectangleFlatButton:
        text:"this is screen 1"
        on_release: app.root.ids.scr_mngr.current = 'screen0'
<Screen2@Screen>:
    name:"screen2"
    MDRectangleFlatButton:
        text:"this is screen 2"
        on_release: app.root.ids.scr_mngr.current = 'screen2'

<Screen3@Screen>:
    name:"screen3"
    MDRectangleFlatButton:
        text:"this is screen 3"
        on_release: app.root.ids.scr_mngr.current = 'screen0'

<MDToolbarKu>:
    title: "Navigation Drawer"
    elevation: 10
    # left_action_items: [['menu', lambda x: self.parent.parent.parent.parent.ids.nav_drawer.toggle_nav_drawer()]]
    left_action_items: [['menu', lambda x: self.parent.ids.nav_drawer.toggle_nav_drawer()]]

