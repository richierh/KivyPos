<CustomIconButton>:
    source:root.source
    id:customiconbutton

<ItemDrawer>:
    theme_text_color: "Custom"
    on_release: self.parent.set_color_item(self)

    IconLeftWidget:
        id: icon
        icon: root.icon
        theme_text_color: "Custom"
        text_color: root.text_color

<ContentNavigationDrawer>:
    navigationmain:root.navigationmain
    id:navigationcontent
    orientation: "vertical"
    padding: "8dp"
    spacing: "8dp"

    AnchorLayout:
        anchor_x: "left"
        size_hint_y: None
        height: avatar.height

        Image:
            id: avatar
            size_hint: None, None
            size: "40dp", "40dp"
            source: "images/avatar.png"

    MDLabel:
        text: "Richie Rahmat Hidayat"
        font_style: "Button"
        size_hint_y: None
        height: self.texture_size[1]

    MDLabel:
        text: "richie130283@gmail.com"
        font_style: "Caption"
        size_hint_y: None
        height: self.texture_size[1]

    ScrollView:

        id:scrollview
        name:'scroll_view'
        DrawerList:
            navigationmain:root.navigationmain
    #         screenmanager:root.screenmanager
    #         nav_drawer:root.nav_drawer
    #         navigationtoolbar:root.navigationtoolbar
    #         mainscreen:root.mainscreen



<DrawerList>:
    id:drawerlist
    mainscreen:root.mainscreen
    navigationmain:root.navigationmain
    # nav_drawer:nav_drawer
    id: md_list
#     name:'drawer_list'
    OneLineIconListItem:
        md_list:md_list
        text:'Cash Register'
        # on_release:root.penjualan() 
        IconLeftWidget:
            icon:'slot-machine'
  
    OneLineIconListItem:
        text:'Produk Tambah'
        # on_release:root.product() 

        IconLeftWidget:
            icon:'bag-suitcase'

    OneLineIconListItem:
        text:'Pengaturan'
        IconLeftWidget:
            icon:'tools'

    OneLineIconListItem:
