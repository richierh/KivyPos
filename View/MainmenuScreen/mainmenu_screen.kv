                            pos_hint: {"top": 1, "right": 1}
                        CustomIconButton:
                            size_hint:.8,.7
                        MDLabel:
                            font_size:'14sp'
                            id: label
                            text: "Event"
                            adaptive_size: True
                            color: "grey"
                            pos: "12dp", "12dp"
                            bold: True
