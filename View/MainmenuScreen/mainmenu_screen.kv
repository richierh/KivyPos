<MainmenuScreenView>
    drawerlist:root.drawerlist
    poskasir:poskasir
    id:navigationmain
    # nav_drawer:nav_drawer
    MDBoxLayout:
        orientation:'vertical'
        MDTopAppBar:
            # pallete:'Red'
            md_bg_color: colors['Purple']['500']
            # pallete:'red'
            # color.pallete:'red'
            id:navigationtoolbar
            title: "Menu Utama"
            elevation: 3
            left_action_items: [['menu', lambda x:nav_drawer.set_state("open")]]
        MDNavigationLayout:
            x: navigationtoolbar.height
            size_hint_y: 1.0 - navigationtoolbar.height/root.height
            ScreenManager:
                id :screenmanager
                PosKasir:
                    drawerlist:root.drawerlist
                    id:poskasir
                    navigationmain:navigationmain
            MDNavigationDrawer:
                id:nav_drawer
                ContentNavigationDrawer:
                    navigationmain:navigationmain
                    id:navigationcontent

<PosKasir>:
    id:root.poskasir
    navigationmain:root.navigationmain
