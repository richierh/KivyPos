<MobileScreenView>
    ScreenManager:
        MainScreen:
            id:mainscreen
            name:'pos screen'
        SecondScreen:
            id:secondscreen
            name:'product screen'

<MainScreen>:
    MDBoxLayout:
        orientation:'vertical'
        padding:[10,10,10,10]

        spacing:5
        orientation:'vertical'

        MDTopAppBar:
            title:'Cash Register'
            right_action_items: 
                [
                ['arrow-left', lambda x: root.go_back()]
                ]
        MDBoxLayout:
            size_hint:1,.5
            spacing:5
            orientation:'horizontal'
            TableCashRegister:
                size_hint:1,1
        MDRaisedButton:
            size_hint:1,None
            text:'hh'
        MDBoxLayout:
            spacing:3
            size_hint:1,None
            orientation:'horizontal'
            height:35
            MDTextFieldRect:
                id:nama_produk
                size_hint:.9,None
                pos_hint: {'center_x': 0.5,'center_y': 0.5}
                height:35
                hint_text:'nama produk'
            MDRaisedButton:
                size_hint:None,None
                text:'pilih'
                pos_hint: {'center_x': 0.5,'center_y': 0.5}
                on_release:root.go_product_image(nama_produk)


        MDBoxLayout:
            orientation:'horizontal'
            size_hint:1,None
            height:35
            spacing:3
